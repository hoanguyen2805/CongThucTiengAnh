<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.app.formulaenglish">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.app.formulaenglish.ui.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.formulaenglish.ui.FormulaEnglishActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.formulaenglish.ui.SentenceChildActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.formulaenglish.ui.DetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.app.formulaenglish.ui.InformationActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>